<link rel="stylesheet" type="text/css" href="<%= File.join(root_url, 'assets/stylesheets', 'fullcalendar.css') %>"/>
<script src="<%= File.join(root_url, 'assets/javascripts', 'fullcalendar.js') %>" type="text/javascript"></script>

<script> 
$(document).ready(function() {
    // page is now ready, initialize the calendar...

$('#calendar').fullCalendar({
	header: {
  	left: 'prev,next today',
  	center: 'title',
  	right: 'month,agendaWeek,agendaDay'
	},
    eventSources: [{
          url: '<%= studio_events_path(@studio) %>',
        }],

      eventColor: 'transparent'
});
</script>

    <div id='calendar'></div>