<!-- Probably move the stylesheet to you layout. Also make sure you include the javascript. -->

<link rel='stylesheet' type='text/css' href='fullcalendar.css' />
<script type='text/javascript' src='jquery.js'></script>
<script type='text/javascript' src='fullcalendar.js'></script>

<script> 
$(document).ready(function() {

  
    // page is now ready, initialize the calendar...

$('#calendar').fullCalendar({
	header: {
  	left: 'prev,next today',
  	center: 'title',
  	right: 'month,agendaWeek,agendaDay'
	},
eventSources: [{
      url: '/studios/1/events'
    },
    {
      url: '/1/calendar'
    }]
});


});
</script>

	<div class="col-lg-2 leftspan" style="margin-top: 10px;">
		<%= render 'layouts/sidebar' %>
	</div>

  <div class="col-lg-8 rightspan">

  <div id='calendar'></div>


    <div class="divider-horizontal-regular"></div>

<%= render 'events/list' %>
</div>