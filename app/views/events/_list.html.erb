<h3>Studio Classes</h3>
<%= link_to 'Create an Event', new_studio_event_path(@studio), class: 'btn btn-sm btn-primary pull-right', :style => "margin-top:-35px;"%>

<% @events = @studio.events.select("DISTINCT title") %>

    <table class="table table-hover">
        <thead>
            <tr>
              <th></th>
              <th>Start Time</th>
              <th>Title</th>
              <th>Location</th>
              <th>Duration</th>
            </tr>       
        </thead>
        <tbody>
            <% @events.each do |event| %>
              <tr>

              <td><%= event.title%></td>
              <td></td>
              <td><%= Event.where(:title => event.title).first.duration %></td>
              </tr>
            <% end %>
        </tbody>
    </table>
